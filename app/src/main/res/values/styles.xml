<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="CommonTabtext" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="CommonDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/common_list_divider_color</item>
    </style>

    <style name="VerticalDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:background">@color/common_list_divider_color</item>
    </style>

    <style name="EatCommonTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxBackgroundColor">@color/common_box_bg</item>
        <item name="boxCornerRadiusBottomEnd">50dp</item>
        <item name="boxCornerRadiusTopEnd">50dp</item>
        <item name="boxCornerRadiusBottomStart">50dp</item>
        <item name="boxCornerRadiusTopStart">50dp</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">0dp</item>
    </style>

    <style name="MyAnimation.Window" parent="@android:style/Animation.Activity">
        <item name="android:windowEnterAnimation">@anim/anim_in</item>
        <item name="android:windowExitAnimation">@anim/anim_out</item>
    </style>

    <style name="WrapImageView" parent="NoDescriptionImageView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="NoDescriptionImageView">
        <item name="android:contentDescription">"@null"</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

    <style name="SubTitleDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/common_list_divider_color</item>
    </style>

